@startuml ATM_Service_UseCases
skinparam monochrome true
skinparam shadowing false
skinparam dpi 150

left to right direction

actor "Инкассатор" as CashCollector

rectangle "Модель банкомата" {

    usecase "Пополнение наличными" as Replenish
    usecase "Изъятие наличных" as Collect
    usecase "Замена кассеты" as ReplaceCassette
}

CashCollector --> Replenish
CashCollector --> Collect
CashCollector --> ReplaceCassette

@enduml